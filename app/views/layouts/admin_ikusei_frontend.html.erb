<!DOCTYPE html>
<html>
<head>
  <%= render partial: 'layouts/admin_ikusei_frontend_head_meta' %>
</head>
<body class="skin-black sidebar-mini fixed">
    <%= render partial: 'layouts/admin_ikusei_frontend_headerbar' %>

    <%# left side column %>
    <%= render partial: 'layouts/admin_ikusei_frontend_sidebar' %>

    <%# page content %>
    <div class="content-wrapper">
      <%= yield %>
    </div>

    <%= render partial: 'layouts/admin_ikusei_frontend_controlbar' %>

    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= yield :page_script %>
  </body>
</html>
