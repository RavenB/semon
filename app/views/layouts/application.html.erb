<!DOCTYPE html>
<html>
<head>
  <%= render "/layouts/partials/admin_ikusei_frontend_head_meta" %>
</head>
<body class="skin-black sidebar-mini fixed">
    <%= render "/layouts/partials/admin_ikusei_frontend_headerbar" %>

    <%# left side column %>
    <%= render "/layouts/partials/admin_ikusei_frontend_sidebar" %>

    <%# page content %>
    <div class="content-wrapper">
      <%= yield %>
    </div>

    <%# create message modal template %>
    <%= render "/messages/partials/create_message_modal" %>

    <%= render "/layouts/partials/admin_ikusei_frontend_controlbar" %>

    <%= javascript_include_tag "application" %>
    <%= yield :page_script %>
  </body>
</html>
